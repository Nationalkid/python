    "import sympy as sp\n",
    "from sympy import *\n",
    "\n",
    "x,y,z = sp.symbols('x, y ,z')\n",
    "\n",
    "f = sp.sin(x * y) + sp.cos(y * z)\n",
    "\n",
    "sp.integrate(f,x)"
